  "private": false,
  "type": "module",
  "description": "Professional A4 PDF generation from HTML using Playwright with precise page control, Cyrillic support, and custom margins",
  "keywords": [
    "pdf",
    "html-to-pdf",
    "playwright",
    "a4",
    "print",
    "pdf-generation",
    "commercial-proposal",
    "cyrillic",
    "page-breaks",
    "paged-media"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/html-to-pdf-generator.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/html-to-pdf-generator/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/html-to-pdf-generator#readme",
  "scripts": {
    "prepare": "playwright install chromium",
    "pdf": "node tools/make-pdf.js",
    "pdf:blue-rakun": "node tools/make-pdf.js --logo=blue-rakun",
    "pdf:spa-bureau": "node tools/make-pdf.js --logo=spa-bureau",
    "pdf:test": "node tools/make-pdf.js --test",
    "install:fonts": "echo Fonts should be placed in src/fonts/"
